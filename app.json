{
  "expo": {
    "name": "instaclone-native",
    "slug": "instaclone-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.googleMaps.googleMaps",
      "config": {
        "googleMaps": { "apiKey": "AIzaSyB_JoQW044ps_-fgRAicl2Sv_ASp8-IUgE" }     
      }
    },  
    
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
